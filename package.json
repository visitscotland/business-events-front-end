    "@vue/eslint-config-airbnb": "^7.0.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "nuxt": "^3.10.2"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "^20.0.0",
    "@bloomreach/vue3-sdk": "^20.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@visitscotland/component-library-export": "^0.0.6",
    "axios": "^1.3.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "mitt": "^3.0.0",
    "nuxt-jsonld": "^2.0.8",
    "pinia": "^2.1.7",
    "sass": "^1.62.0",
    "standard-version": "^9.5.0",
    "youtube-vue3": "^0.1.15"
  },
  "overrides": {
    "vue": "latest"
