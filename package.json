    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "nuxt": "^3.4.1"
  },
  "dependencies": {
    "@bloomreach/spa-sdk": "^20.0.0",
    "@bloomreach/vue3-sdk": "^20.0.0",
    "axios": "^1.3.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "sass": "^1.62.0",
    "standard-version": "^9.5.0"
  },
  "overrides": {
    "vue": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
